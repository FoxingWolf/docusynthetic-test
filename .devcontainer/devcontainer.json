{
  "name": "Venice KB Collector",
  "image": "mcr.microsoft.com/devcontainers/python:3.11",
  "features": {
    "ghcr.io/devcontainers/features/git:1": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-python.black-formatter"
      ]
    }
  },
  "postCreateCommand": "make setup && make install",
  "forwardPorts": [],
  "remoteEnv": {
    "PYTHONUNBUFFERED": "1"
  }
}
