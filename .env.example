# Venice API Key (optional - required for API probing and LLM features)
VENICE_API_KEY=your_api_key_here

# Cache settings
CACHE_DIR=.cache
CACHE_ENABLED=true

# Chunk settings
CHUNK_MAX_TOKENS=3500

# Build settings
SKIP_WEB_SCRAPING=false
SKIP_API_PROBING=false
SKIP_LLM_FEATURES=false
FORCE_REFRESH=false

# GitHub settings (usually not needed - uses public API)
# GITHUB_TOKEN=your_github_token_here
